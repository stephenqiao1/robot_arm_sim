<?xml version="1.0"?>
<robot name="colored_cubes">

    <!-- Green Cube -->
    <link name="green_cube">
        <inertial>
            <mass value="1" />
            <inertia ixx="0.1" ixy="0" ixz="0" iyy="0.1" iyz="0" izz="0.1" />
            <origin xyz="0 0 0" rpy="0 0 0" />
        </inertial>
        <visual>
            <origin xyz="0.5 0.5 0.05" rpy="0 0 0" />
            <geometry>
                <box size="0.1 0.1 0.1" />
            </geometry>
            <material name="green">
                <color rgba="0 1 0 1" />
            </material>
        </visual>
        <collision>
            <origin xyz="0.5 0.5 0.05" rpy="0 0 0" />
            <geometry>
                <box size="0.1 0.1 0.1" />
            </geometry>
        </collision>
    </link>

        <!-- Blue Cube -->
        <link name="blue_cube">
            <inertial>
                <mass value="1" />
                <inertia ixx="0.1" ixy="0" ixz="0" iyy="0.1" iyz="0" izz="0.1" />
                <origin xyz="0 0 0" rpy="0 0 0" />
            </inertial>
            <visual>
                <origin xyz="-0.5 -0.5 0.05" rpy="0 0 0" />
                <geometry>
                    <box size="0.1 0.1 0.1" />
                </geometry>
                <material name="blue">
                    <color rgba="0 0 1 1" />
                </material>
            </visual>
            <collision>
                <origin xyz="-0.5 -0.5 0.05" rpy="0 0 0" />
                <geometry>
                    <box size="0.1 0.1 0.1" />
                </geometry>
            </collision>
        </link>

        <!-- Red Cube -->
        <link name="red_cube">
            <inertial>
                <mass value="1" />
                <inertia ixx="0.1" ixy="0" ixz="0" iyy="0.1" iyz="0" izz="0.1" />
                <origin xyz="0 0 0" rpy="0 0 0" />
            </inertial>
            <visual>
                <origin xyz="0 -0.5 0.05" rpy="0 0 0" />
                <geometry>
                    <box size="0.1 0.1 0.1" />
                </geometry>
                <material name="red">
                    <color rgba="1 0 0 1" />
                </material>
            </visual>
            <collision>
                <origin xyz="0 -0.5 0.05" rpy="0 0 0" />
                <geometry>
                    <box size="0.1 0.1 0.1" />
                </geometry>
            </collision>
        </link>
</robot>